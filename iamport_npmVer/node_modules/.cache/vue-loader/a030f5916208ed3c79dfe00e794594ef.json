\r\n          v-decorator=\"['company', { initialValue: 'SIOT' }]\"\r\n          size=\"large\"\r\n        />\r\n      </a-form-item>\r\n      <a-form-item label=\"통신사\">\r\n        <a-select\r\n          v-decorator=\"['carrier', { initialValue: 'SKT' }]\"\r\n          size=\"large\"\r\n        >\r\n          <a-select-option value=\"SKT\">\r\n            SKT\r\n          </a-select-option>\r\n          <a-select-option value=\"KTF\">\r\n            KT\r\n          </a-select-option>\r\n          <a-select-option value=\"LGT\">\r\n            LGU+\r\n          </a-select-option>\r\n          <a-select-option value=\"MVNO\">\r\n            알뜰폰\r\n          </a-select-option>\r\n        </a-select>\r\n      </a-form-item>\r\n      <a-form-item label=\"이름\">\r\n        <a-input\r\n          v-decorator=\"['name']\"\r\n          size=\"large\"\r\n        />\r\n      </a-form-item>\r\n      <a-form-item label=\"전화번호\">\r\n        <a-input\r\n          v-decorator=\"['phone']\"\r\n          type=\"number\"\r\n          size=\"large\"\r\n        />\r\n      </a-form-item>\r\n      <a-form-item label=\"허용최소연령\">\r\n        <a-input\r\n          v-decorator=\"['minAge']\"\r\n          type=\"number\"\r\n          size=\"large\"\r\n        />\r\n      </a-form-item>\r\n      <a-button size=\"large\" @click=\"handleGoBack\">\r\n        뒤로가기\r\n      </a-button>\r\n      <a-button type=\"primary\" html-type=\"submit\" size=\"large\">\r\n        본인인증\r\n      </a-button>\r\n    </a-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      formLayout: 'horizontal',\r\n      form: this.$form.createForm(this, { name: 'certification' }),\r\n      initialMerchantUid: `mid_${new Date().getTime()}`,\r\n    };\r\n  },\r\n  methods: {\r\n    handleSubmit(e) {\r\n      e.preventDefault();\r\n      this.form.validateFields((err, values) => {\r\n        if (!err) {\r\n          const {\r\n            merchantUid, company, carrier, name, phone, minAge,\r\n          } = values;\r\n          const { IMP } = window;\r\n          IMP.init('imp10391932');\r\n          const data = {\r\n            merchant_uid: merchantUid,\r\n            company,\r\n            carrier,\r\n            name,\r\n            phone,\r\n            min_age: minAge,\r\n          };\r\n          IMP.certification(data, this.callback);\r\n        }\r\n      });\r\n    },\r\n    handleGoBack() {\r\n      this.$router.push('/');\r\n    },\r\n    callback(response) {\r\n      // 본인인증 종료 후 result 페이지로 이동\r\n      const query = {\r\n        ...response,\r\n        type: 'certification',\r\n      };\r\n      this.$router.push({ path: '/result', query });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}